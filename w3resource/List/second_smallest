def second_smallest(numbers):
    if len(numbers) < 2:
        return None
    unique_numbers = list(set(numbers))
    unique_numbers.sort()
    return unique_numbers[1] if len(unique_numbers) >= 2 else None
data = [12,3,4,5,2]
result = second_smallest(data)
print(f"Second smallest numner is : {result}")